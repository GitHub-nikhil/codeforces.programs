#include <iostream>
#include <algorithm>
#include <vector>
#include <unordered_map>
#include <math.h>
#include <iomanip>
#include <map>
#include <set>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,k,ans = 0;
        cin>>n>>k;
        if(n%k == 0) ans = 1;
        else if(n%k!=0 && n>k) ans = 2;
        else if(k%n == 0) ans = k/n;
        else if(k>n) ans = (k/n)+1;
        cout<<ans<<endl;
        }
        return 0;
    }
