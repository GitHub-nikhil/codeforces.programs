#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
#include <map>
#include <vector>
#include <stack>
#include <unordered_map>
#include <unordered_set>
#define ll long long int
#define ull unsigned long long int
#define mod 1000000007
#define loop(n) for(int i = 0;i<n;i++) cout<<arr[i]<<" ";
#define loopIN(n) for(int i = 0;i<n;i++) cin>>arr[i];
#define nextln cout<<endl;
using namespace std;
 
void solve(){
    ll n;
    cin>>n;
    ll arr[n+1];
    for(int i = 1;i<=n;i++){
        cin>>arr[i];
        if(i%2 == 1){
            if(arr[i]<0) arr[i] = -1*arr[i];
            }
            else{
                if(arr[i]>0) arr[i] = -1*arr[i];
                }
        }
        for(int i = 1;i<=n;i++) cout<<arr[i]<<" ";
        cout<<endl;
        
        return;
    }
int main(){
    int t=1;
    cin>>t;
    for(int i = 1;i<=t;i++){
        solve();
        }
    
    return 0;
    }
