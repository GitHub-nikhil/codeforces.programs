void solve(int i){
ll n,m;
cin>>n>>m;
if(n == 1 || m== 1) cout<<"YES"<<endl;
else if(n == 2 && m == 2) cout<<"YES"<<endl;
else cout<<"NO"<<endl;
 
return;
}
 
int main(){
ll t;
cin>>t;
for(int i = 1;i<=t;i++){
solve(i);
}
 
return 0;
}
