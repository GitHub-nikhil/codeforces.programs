#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
#include <map>
#include <vector>
using namespace std;
 
int main(){
    int t;
    cin>>t;
    while(t--){
        long long int n;
        cin>>n;
        long long int equal = n/3;
        long long int rem = n%3;
        if(rem == 0) cout<<equal<<" "<<equal<<endl;
        else if(rem == 2) cout<<equal<<" "<<equal+1<<endl;
        else if(rem == 1) cout<<equal+1<<" "<<equal<<endl;
        }
        return 0;
    }
