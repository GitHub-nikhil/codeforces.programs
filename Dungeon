#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        long long int a,b,c;
        cin>>a>>b>>c;
        long long int sum = (a+b+c)/9;
        if((a+b+c)%9 == 0 && a>=sum && b>=sum && c>=sum) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        }
        return 0;
    }
