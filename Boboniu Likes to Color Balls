#define ll long long int
#define ull unsigned long long int
#define mod 1000000007
#define loop(n) for(int i = 0;i<n;i++) cout<<arr[i]<<" ";
#define loopIN(n) for(int i = 0;i<n;i++) cin>>arr[i];
#define nextln cout<<endl;
using namespace std;
//Forget Ratings and Enjoy the Contest
void solve(){
    int r,g,b,w,odd = 0;
    cin>>r>>g>>b>>w;
    if(r%2 == 1) odd++;
    if(g%2 == 1) odd++;
    if(b%2 == 1) odd++;
    if(w%2 == 1) odd++;
    
    if(odd == 3 && (r == 0 || g == 0 || b == 0)) cout<<"No"<<endl;
    else if(odd == 2) cout<<"No"<<endl;
    else cout<<"Yes"<<endl;
    }
    
int main(){
    ll t;
    cin>>t;
    while(t--){
        solve();
        }
        return 0;
    }
