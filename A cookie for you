int main(){
    int t;
    cin>>t;
    while(t--){
        ll a,b,n,m;
        cin>>a>>b>>n>>m;
        if(a+b<n+m) cout<<"NO"<<endl;
        else{
            if(m<=min(a,b)) cout<<"YES"<<endl;
            else cout<<"NO"<<endl;
            }
        }
        return 0;
    }
