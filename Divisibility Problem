#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
#include <map>
#include <vector>
using namespace std;
 
int main(){
    int t;
    cin>>t;
    while(t--){
        int a,b;
        cin>>a>>b;
        if(b>a){
            cout<<b-a<<endl;
            }
            else{
        int rem = a%b;
        if(rem == 0) cout<<0<<endl;
        else{
            int divi = a/b;
            divi++;
            int req = b*divi;
            cout<<req-a<<endl;
            }
        }
    }
        return 0;
    }
