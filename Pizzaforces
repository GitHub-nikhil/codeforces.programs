#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
#include <map>
#include <vector>
#include <unordered_map>
#define ll long long int
using namespace std;
 
//PizzaForces
int main(){
    int t;
    cin>>t;
    while(t--){
    ll n;
    cin>>n;
    if(n<=6) cout<<15<<endl;
    else if(n%2!=0){
          ll ans = (n+1)*2.5;
          cout<<ans<<endl;
          }
          else{
              ll ans = n*2.5;
              cout<<ans<<endl;
              }
      }
      return 0;
    }
