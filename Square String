//#define loopIN(n) for(int i = 0;i<n;i++) cin>>arr[i];
//#define nextln cout<<"\n";
using namespace std;

void solve(){
    string s,first,second;
    cin>>s;
    
    if(s.length()%2 == 1) cout<<"NO"<<endl;
    else{
        for(int i = 0;i<s.length();i++){
        if(i<s.length()/2) first = first+s[i];
        else second = second+s[i];
        }
        
        if(first == second) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        }
        
        return;
    }
    
int main(){
ios::sync_with_stdio(false);
cin.tie(0);
ll t = 1;
cin>>t;
for(int i = 1;i<=t;i++){
solve();
}

return 0;
}
